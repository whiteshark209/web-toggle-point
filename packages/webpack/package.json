{
  "name": "@asos/web-toggle-point-webpack",
  "description": "toggle point webpack plugin",
  "version": "0.7.2",
  "license": "MIT",
  "type": "module",
  "main": "./lib/main.cjs",
  "exports": {
    "./plugins": {
      "import": "./lib/main.js",
      "require": "./lib/main.cjs"
    },
    "./pathSegmentToggleHandler": "./lib/pathSegmentToggleHandler.js"
  },
  "scripts": {
    "build": "npm run clean && rollup -c build/rollup.mjs",
    "prepublishOnly": "npm run build",
    "clean": "rimraf ./lib",
    "docs": "rimraf ./docs/**/*.html && jsdoc -c ../jsdoc.conf.js",
    "lint": "eslint build src docs --flag unstable_config_lookup_from_file",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "fast-glob": "^3.2.12",
    "regexgen": "^1.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/webpack-env": "^1.18.0",
    "eslint-plugin-jsdoc": "^50.5.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "jsdoc-tsimport-plugin": "^1.0.5",
    "memfs": "^4.15.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "schema-utils": "^4.2.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0",
    "webpack-test-utils": "^2.1.0"
  },
  "peerDependencies": {
    "webpack": ">=5.70",
    "next": ">14"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  }
}
