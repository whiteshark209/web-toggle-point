    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.0.3",
    "@types/mdx": "^2.0.13",
    "next": "^15.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.0.3",
    "eslint-config-next": "^15.0.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-mdx": "^3.1.5",
    "path-exists-cli": "^2.0.0"
  },
