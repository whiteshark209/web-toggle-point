    "lint:code": "next lint",
    "lint:docs": "eslint **/*.mdx"
  },
  "dependencies": {
    "@asos/web-toggle-point-react-pointcuts": "file:../../packages/react-pointcuts",
    "@asos/web-toggle-point-features": "file:../../packages/features",
